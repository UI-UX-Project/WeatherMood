{
  "expo": {
    "name": "WeatherMood",
    "slug": "WeatherMood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/background-blur.png",
      "resizeMode": "cover",
      "backgroundColor": "#393E6B"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
